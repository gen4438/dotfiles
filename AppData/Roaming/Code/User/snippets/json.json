	"VSCode Color Bar Example": {
		"prefix": "vscode-color-bar",
		"body": [
			"{",
			"    \"workbench.colorCustomizations\": {",
			"        \"titleBar.activeBackground\": \"#E95420\",",
			"        \"titleBar.inactiveBackground\": \"#C34113\",",
			"        \"titleBar.activeForeground\": \"#FFFFFF\",",
			"        \"titleBar.inactiveForeground\": \"#FFFFFF\",",
			"        \"activityBar.background\": \"#E95420\",",
			"        \"activityBar.foreground\": \"#FFFFFF\"",
			"    }",
			"}"
		],
		"description": "VSCodeのタイトルバーとアクティビティバーの色を変更する例"
	},
	"DevContainer Rootless Docker remoteEnv": {
		"prefix": "devcontainer-remoteenv-rootless-docker",
		"body": [
			"\"remoteEnv\": {",
			"    \"DOCKER_HOST\": \"unix:///run/user/${UID}/docker.sock\"",
			"},"
		],
		"description": "devcontainer.json用 rootless Docker向けremoteEnvオプション"
	}
}