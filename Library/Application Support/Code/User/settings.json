{
  "[glsl]": {
    "editor.defaultFormatter": "raczzalan.webgl-glsl-editor"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "dev.containers.dockerSocketPath": "unix:///run/user/${UID}/docker.sock",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "code-runner.enableAppInsights": false,
  "code-runner.runInTerminal": true,
  "database-client.autoSync": true,
  "database-client.telemetry.usesOnlineServices": false,
  "editor.cursorStyle": "block",
  "editor.scrollBeyondLastLine": false,
  "editor.selectionHighlight": false,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?、。「」【】『』（）！？てにをはがのともへでや",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "gitlens.telemetry.enabled": false,
  "path-intellisense.autoSlashAfterDirectory": true,
  "path-intellisense.showHiddenFiles": true,
  "python.languageServer": "Pylance",
  "redhat.telemetry.enabled": false,
  "remote.SSH.remotePlatform": {
    "minis": "linux"
  },
  "settingsSync.ignoredSettings": [
    "remote.SSH.configFile",
    "remote.SSH.remotePlatform",
    "dev.containers.dockerSocketPath"
  ],
  "settingsSync.keybindingsPerPlatform": false,
  "telemetry.editStats.enabled": false,
  "telemetry.feedback.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.commandsToSkipShell": [
    "-workbench.action.quickOpen",
    "-workbench.action.terminal.findNext",
    "-workbench.action.terminal.findPrevious",
    "-workbench.action.debug.continue",
    "-workbench.action.debug.restart",
    "-workbench.action.debug.run",
    "-workbench.action.debug.start",
    "-workbench.action.debug.stop",
    "-workbench.action.focusNextPart",
    "-workbench.action.focusPreviousPart",
    "-workbench.action.debug.pause",
    "-workbench.action.quickOpen",
    "-workbench.action.terminal.focusFind"
  ],
  "vscode-neovim.compositeKeys": {},
  "vscode-neovim.ctrlKeysForInsertMode": [
    "a",
    "c",
    "d",
    "h",
    "f",
    "j",
    "l",
    "n",
    "m",
    "o",
    "p",
    "r",
    "s",
    "t",
    "u",
    "w",
    "x",
    "y"
  ],
  "vscode-neovim.ctrlKeysForNormalMode": [
    "a",
    "c",
    "d",
    "e",
    "f",
    "h",
    "i",
    "j",
    "l",
    "m",
    "o",
    "p",
    "r",
    "t",
    "u",
    "v",
    "w",
    "x",
    "y",
    "z",
    "/",
    "]"
  ],
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "vscode-neovim.neovimExecutablePaths.win32": "C:\\Program Files\\Neovim\\bin\\nvim.exe",
  "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init.lua",
  "vscode-neovim.neovimInitVimPaths.linux": "~/.config/nvim/init.lua",
  "vscode-neovim.neovimInitVimPaths.win32": "~/AppData/Local/nvim/init.lua",
  "workbench.colorTheme": "GitHub Dark High Contrast",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorCustomizations": {},
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "claudeCodeChat.wsl.enabled": false,
  "claudeCodeChat.wsl.distro": "Ubuntu",
  "claudeCodeChat.wsl.nodePath": "/usr/bin/node",
  "claudeCodeChat.wsl.claudePath": "/usr/local/bin/claude"
}