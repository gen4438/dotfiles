{
  "[glsl]": {
    "editor.defaultFormatter": "raczzalan.webgl-glsl-editor"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "chat.viewSessions.orientation": "stacked",
  "claudeCode.allowDangerouslySkipPermissions": true,
  "claudeCode.preferredLocation": "panel",
  "claudeCode.selectedModel": "sonnet",
  "code-runner.enableAppInsights": false,
  "code-runner.runInTerminal": true,
  "database-client.autoSync": true,
  "database-client.telemetry.usesOnlineServices": false,
  "dev.containers.forwardWSLServices": false,
  "diffEditor.codeLens": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.cursorStyle": "block",
  "editor.scrollBeyondLastLine": false,
  "editor.selectionHighlight": false,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?、。「」【】『』（）！？てにをはがのともへでや",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "geminicodeassist.enableTelemetry": false,
  "geminicodeassist.project": "starry-goal-pdpsm",
  "git.autofetch": true,
  "git.blame.editorDecoration.enabled": false,
  "git.blame.statusBarItem.enabled": true,
  "git.confirmSync": false,
  "github.copilot.chat.agent.thinkingTool": true,
  "github.copilot.chat.codesearch.enabled": true,
  "github.copilot.chat.commitMessageGeneration.instructions": [
    {
      "text": "コミットメッセージは日本語で書いてください。commitizenの規約に従ってください。"
    },
  ],
  "github.copilot.chat.editor.temporalContext.enabled": true,
  "github.copilot.chat.edits.temporalContext.enabled": true,
  "github.copilot.chat.generateTests.codeLens": true,
  "github.copilot.chat.languageContext.fix.typescript.enabled": true,
  "github.copilot.chat.languageContext.inline.typescript.enabled": true,
  "github.copilot.chat.languageContext.typescript.enabled": true,
  "github.copilot.chat.notebook.followCellExecution.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": true,
    "scminput": false
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "githubPullRequests.experimental.useQuickChat": true,
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "gitlens.telemetry.enabled": false,
  "gitlens.views.branches.branches.layout": "list",
  "gitlens.views.scm.grouped.hiddenViews": {
    "branches": false,
    "commits": false,
    "contributors": false,
    "fileHistory": false,
    "launchpad": false,
    "remotes": false,
    "repositories": true,
    "searchAndCompare": false,
    "stashes": false,
    "tags": false,
    "worktrees": true
  },
  "makefile.configureOnOpen": true,
  "path-intellisense.autoSlashAfterDirectory": true,
  "path-intellisense.showHiddenFiles": true,
  "python.analysis.addHoverSummaries": true,
  "python.analysis.typeCheckingMode": "standard",
  "python.languageServer": "Pylance",
  "redhat.telemetry.enabled": false,
  "remote.autoForwardPortsSource": "hybrid",
  "remote.SSH.remotePlatform": {
    "minis": "linux"
  },
  "settingsSync.ignoredExtensions": [
    "smcpeak.default-keys-windows"
  ],
  "settingsSync.ignoredSettings": [
    "remote.SSH.configFile",
    "remote.SSH.remotePlatform",
    "dev.containers.dockerSocketPath"
  ],
  "settingsSync.keybindingsPerPlatform": false,
  "telemetry.editStats.enabled": false,
  "telemetry.feedback.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.commandsToSkipShell": [
    "-workbench.action.quickOpen",
    "-workbench.action.terminal.findNext",
    "-workbench.action.terminal.findPrevious",
    "-workbench.action.debug.continue",
    "-workbench.action.debug.restart",
    "-workbench.action.debug.run",
    "-workbench.action.debug.start",
    "-workbench.action.debug.stop",
    "-workbench.action.focusNextPart",
    "-workbench.action.focusPreviousPart",
    "-workbench.action.debug.pause",
    "-workbench.action.quickOpen",
    "-workbench.action.terminal.focusFind"
  ],
  "vscode-edge-devtools.headless": false,
  "vscode-neovim.compositeKeys": {},
  "vscode-neovim.ctrlKeysForInsertMode": [
    "a",
    "c",
    "d",
    "h",
    "f",
    "j",
    "l",
    "n",
    "m",
    "o",
    "p",
    "r",
    "s",
    "t",
    "u",
    "w",
    "x",
    "y"
  ],
  "vscode-neovim.ctrlKeysForNormalMode": [
    "a",
    "c",
    "d",
    "e",
    "f",
    "h",
    "i",
    "j",
    "l",
    "m",
    "o",
    "p",
    "r",
    "t",
    "u",
    "v",
    "w",
    "x",
    "y",
    "z",
    "/",
    "]"
  ],
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "vscode-neovim.neovimExecutablePaths.win32": "C:\\Program Files\\Neovim\\bin\\nvim.exe",
  "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init.lua",
  "vscode-neovim.neovimInitVimPaths.linux": "~/.config/nvim/init.lua",
  "vscode-neovim.neovimInitVimPaths.win32": "~/AppData/Local/nvim/init.lua",
  "workbench.colorCustomizations": {},
  "workbench.editor.wrapTabs": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.defaultFindMode": "filter",
  "workbench.tree.indent": 20,
  "oil-code.disableVimKeymaps": true
}